				<annotation>
					<documentation xml:lang="es">
						10 - 2.1.10 Datos Términos de Entrega:
						Agrupación de campos relativos al transporte o
						entrega de los bienes. Se presenta nuevos
						participantes relativos a la entrega, como el
						transportista
					</documentation>
				</annotation>
			</element>
			<element ref="tns:DeliveryTerms" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation xml:lang="es">
						2.2.10 - Medios de Transporte
					</documentation>
				</annotation>
			</element>
			<element ref="cac:PaymentMeans" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation xml:lang="es">
						12.1 - Datos del Pago: Información relativa al
						pago de la factura.
					</documentation>
				</annotation>
			</element>
			<element ref="cac:PaymentTerms" minOccurs="0" maxOccurs="unbounded">
